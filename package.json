{
  "name": "nestjs-nuxt-starter",
  "version": "0.0.3",
  "description": "A nest nuxt starter",
  "author": "Goopil <me@goopil.org>",
  "license": "MIT",
  "private": true,
  "main": "dist/server/main.js",
  "scripts": {
    "prebuild": "rimraf dist",
    "predev": "rimraf dist",
    "build:front": "cross-env NODE_ENV=production NODE_OPTIONS=--openssl-legacy-provider nuxt build -c server/nuxt/nuxt.config.ts",
    "build:back": "cross-env mode=production NODE_ENV=production NODE_OPTIONS=--openssl-legacy-provider nest build --webpack --webpackPath webpack.config.js",
    "build": "concurrently \"yarn build:back\" \"yarn build:front\"",
    "dev": "cross-env NODE_ENV=development NODE_OPTIONS=--openssl-legacy-provider nest build --webpack --webpackPath webpack.config.js --watch",
    "start": "node dist/server/main",
    "lint": "eslint \"{common,test,server,client}/**/*.{js,ts,vue}\"",
    "format": "yarn run lint --fix",
    "test": "jest --config test/jest.config.js --passWithNoTests",
    "test:watch": "yarn test --watch",
    "test:cov": "yarn test --coverage",
    "e2e": "jest --config ./test/jest-e2e.config.js --passWithNoTests",
    "e2e:watch": "yarn e2e --watch",
    "e2e:cov": "yarn e2e --coverage",
    "audit:fix": "npx yarn-audit-fix"
  },
  "dependencies": {
    "@nestjs/common": "9.1.2",
    "@nestjs/core": "9.1.2",
    "@nestjs/platform-express": "9.1.2",
    "@nestjs/platform-fastify": "9.1.2",
    "@nuxtjs/axios": "5.13.6",
    "nuxt": "2.15.8",
    "nuxt-precompress": "0.5.9",
    "nuxt-property-decorator": "2.9.1",
    "reflect-metadata": "0.1.13",
    "rxjs": "7.5.6",
    "tslib": "2.4.0"
  },
  "devDependencies": {
    "@nestjs/cli": "9.1.3",
    "@nestjs/schematics": "9.0.3",
    "@nestjs/testing": "9.1.2",
    "@nuxt/types": "2.15.8",
    "@nuxt/typescript-build": "2.1.0",
    "@types/connect": "3.4.35",
    "@types/express": "4.17.14",
    "@types/jest": "29.0.3",
    "@types/node": "18.7.21",
    "@types/supertest": "2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.38.0",
    "@typescript-eslint/parser": "^5.38.0",
    "clean-webpack-plugin": "4.0.0",
    "concurrently": "7.4.0",
    "cross-env": "7.0.3",
    "eslint": "^8.24.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vue": "^9.5.1",
    "jest": "29.0.3",
    "prettier": "2.7.1",
    "rimraf": "3.0.2",
    "run-script-webpack-plugin": "0.0.11",
    "sass": "1.55.0",
    "supertest": "6.2.4",
    "ts-jest": "29.0.2",
    "ts-loader": "8.3.0",
    "typescript": "4.8.3",
    "webpack": "4.46.0",
    "webpack-cli": "4.10.0",
    "webpack-node-externals": "3.0.0"
  },
  "resolutions": {
    "ansi-html": ">=0.0.8"
  }
}
