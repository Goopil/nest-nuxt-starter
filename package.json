{
  "name": "nestjs-nuxt-starter",
  "version": "0.0.2",
  "description": "a nest nuxt starter",
  "author": "Goopil <me@goopil.org>",
  "license": "MIT",
  "private": true,
  "main": "dist/server/main.js",
  "scripts": {
    "prebuild": "rimraf dist",
    "predev": "rimraf dist",
    "build": "cross-env mode=production NODE_ENV=production concurrently \"nuxt build\" \"nest build\"",
    "format": "prettier --write \"server/**/*.ts\" \"client/**/*\" \"test/**/*.ts\"",
    "dev": "cross-env NODE_ENV=development webpack --config webpack-hmr.config.js",
    "start": "node dist/server/main",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "test": "jest --passWithNoTests",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage --passWithNoTests",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "snyk-protect": "snyk protect"
  },
  "dependencies": {
    "@nestjs/common": "8.0.6",
    "@nestjs/core": "8.0.6",
    "@nestjs/platform-express": "8.0.6",
    "@nestjs/platform-fastify": "8.0.6",
    "@nuxtjs/axios": "5.13.6",
    "nuxt": "2.15.8",
    "nuxt-property-decorator": "2.9.1",
    "reflect-metadata": "0.1.13",
    "rxjs": "7.3.0",
    "tslib": "2.3.1"
  },
  "devDependencies": {
    "sass": "1.39.0",
    "run-script-webpack-plugin": "0.0.11",
    "@nestjs/cli": "8.1.1",
    "@nestjs/schematics": "8.0.3",
    "@nestjs/testing": "8.0.6",
    "@nuxt/types": "2.15.8",
    "@nuxt/typescript-build": "2.1.0",
    "@types/connect": "3.4.35",
    "@types/express": "4.17.13",
    "@types/jest": "27.0.1",
    "@types/node": "16.7.10",
    "@types/supertest": "2.0.11",
    "cross-env": "7.0.3",
    "fork-ts-checker-webpack-plugin": "6.3.3",
    "jest": "27.1.0",
    "prettier": "2.3.2",
    "rimraf": "3.0.2",
    "snyk": "1.696.0",
    "supertest": "6.1.6",
    "ts-jest": "27.0.5",
    "ts-loader": "8.3.0",
    "ts-node": "10.2.1",
    "tsconfig-paths": "3.11.0",
    "tsconfig-paths-webpack-plugin": "3.5.1",
    "tslint": "6.1.3",
    "typescript": "4.4.2",
    "webpack": "4.46.0",
    "webpack-cli": "4.8.0",
    "webpack-node-externals": "3.0.0",
    "concurrently": "6.2.1"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".spec.ts$",
    "transform": {
      ".+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "./coverage",
    "testEnvironment": "node"
  },
  "snyk": true
}
