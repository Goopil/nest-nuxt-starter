{
  "name": "nestjs-nuxt-starter",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "license": "MIT",
  "main": "dist/server/main.js",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "cross-env mode=production NODE_ENV=production yarn prebuild && nuxt-ts build && nest build",
    "format": "prettier --write \"server/**/*.ts\" \"client/**/*\" \"test/**/*.ts\"",
    "start:dev": "cross-env NODE_ENV=development yarn prebuild && webpack --config webpack-hmr.config.js",
    "start": "cross-env NODE_ENV=production node dist/server/main",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "test": "jest --passWithNoTests",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage --passWithNoTests",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "snyk-protect": "snyk protect"
  },
  "dependencies": {
    "@nestjs/common": "7.0.9",
    "@nestjs/core": "7.0.9",
    "@nestjs/platform-express": "7.0.9",
    "@nestjs/platform-fastify": "7.0.9",
    "@nuxtjs/axios": "5.10.3",
    "nuxt": "2.12.2",
    "nuxt-property-decorator": "2.7.2",
    "reflect-metadata": "0.1.13",
    "rxjs": "6.5.5",
    "snyk": "1.336.0"
  },
  "devDependencies": {
    "@nestjs/cli": "7.1.5",
    "@nestjs/schematics": "7.0.0",
    "@nestjs/testing": "7.0.9",
    "@nuxt/typescript-build": "0.6.6",
    "@nuxt/typescript-runtime": "0.4.6",
    "@types/connect": "3.4.33",
    "@types/express": "4.17.6",
    "@types/jest": "25.2.1",
    "@types/node": "13.13.5",
    "@types/supertest": "2.0.9",
    "cross-env": "7.0.2",
    "fork-ts-checker-webpack-plugin": "4.1.3",
    "jest": "26.0.1",
    "prettier": "2.0.5",
    "rimraf": "3.0.2",
    "supertest": "4.0.2",
    "ts-jest": "25.5.1",
    "ts-loader": "7.0.3",
    "ts-node": "8.10.1",
    "tsconfig-paths": "3.9.0",
    "tsconfig-paths-webpack-plugin": "3.2.0",
    "tslint": "6.1.2",
    "typescript": "3.8.3",
    "webpack-cli": "3.3.11",
    "webpack-node-externals": "1.7.2",
    "webpack-shell-plugin": "0.5.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".spec.ts$",
    "transform": {
      ".+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "./coverage",
    "testEnvironment": "node"
  },
  "snyk": true
}
